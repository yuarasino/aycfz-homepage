{
  "name": "aycfz-homepage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint --ignore-path .gitignore '**/*.{js,ts,tsx}'",
    "lint:prettier": "prettier --check --ignore-path .gitignore '**/*.{js,ts,tsx}'",
    "fix": "yarn fix:eslint && yarn fix:prettier",
